datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id        String   @id @default(uuid())
  nome      String
  email     String   @unique
  patente   String
  unidade   String
  senha     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}