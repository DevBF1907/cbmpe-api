{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "testTimeout": 30000,
  "maxWorkers": 1,
  "transform": {
    "^.+\\.(t|j)s$": ["ts-jest", {
      "tsconfig": "<rootDir>/tsconfig.json"
    }]
  },
  "setupFilesAfterEnv": ["<rootDir>/setup-e2e.ts"]
}
